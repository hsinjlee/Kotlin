fun main() {  
  var x = 100 + 50
  println(x)  
}
==
150
==

fun main() {  
  var sum1 = 100 + 50        // 150 (100 + 50)
  var sum2 = sum1 + 250      // 400 (150 + 250)
  var sum3 = sum2 + sum2     // 800 (400 + 400)
  println(sum1)
  println(sum2)
  println(sum3)  
}
==
150
400
800
==

fun main() {  
  var x = 10
  println(x)  
}
==
10
==

fun main() {  
  var x = 10
  x += 5
  println(x)  
}
==
15
==

