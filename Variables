fun main() {
  var name = "John"
  val birthyear = 1975
  
  println(name)
  println(birthyear)
}
==
John
1975
==

fun main() {
  var name = "John" // String
  val birthyear = 1975 // Int
  println(name)
  println(birthyear)
}
==
John
1975
==

fun main() {
  var name: String
  name = "John"
  println(name)
}
==
John
==

fun main() {
  var name
  name = "John"
  println(name)
}
==
