fun main() {
  val name = "John"
  name = "Robert"
  println(name)
}
==
prog.kt:3:3: error: val cannot be reassigned
  name = "Robert"
  ^
==

fun main() {
  var name = "John"
  name = "Robert"
  println(name)
}
==
Robert
==

fun main() {
  val pi = 3.14159265359
  println(pi)
}
==
3.14159265359
==

fun main() {
  val name = "John"
  println("Hello " + name)
}
==
Hello John
==

fun main() {
  val firstName = "John "
  val lastName = "Doe"
  val fullName = firstName + lastName
  println(fullName)
}
==
John Doe
==

fun main() {
  val x = 5
  val y = 6
  println(x + y) // Print the value of x + y
}
==
11
==


  
